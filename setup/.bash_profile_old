if [ -f ~/.bashrc ]; then . ~/.bashrc; fi

export PS1="\u: \w/$ "

# added by Anaconda3 5.1.0 installer
# export PATH="/Users/plavrov/anaconda3/bin:$PATH"  # commented out by conda initialize

# >>> conda initialize >>>
# !! Contents within this block are managed by 'conda init' !!
__conda_setup="$('/Users/plavrov/anaconda3/bin/conda' 'shell.bash' 'hook' 2> /dev/null)"
if [ $? -eq 0 ]; then
    eval "$__conda_setup"
else
    if [ -f "/Users/plavrov/anaconda3/etc/profile.d/conda.sh" ]; then
        . "/Users/plavrov/anaconda3/etc/profile.d/conda.sh"
    else
        export PATH="/Users/plavrov/anaconda3/bin:$PATH"
    fi
fi
unset __conda_setup
# <<< conda initialize <<<

# added by Anaconda3 2019.07 installer
# >>> conda init >>>
# !! Contents within this block are managed by 'conda init' !!
__conda_setup="$(CONDA_REPORT_ERRORS=false '/Users/plavrov/anaconda3/bin/conda' shell.bash hook 2> /dev/null)"
if [ $? -eq 0 ]; then
    \eval "$__conda_setup"
else
    if [ -f "/Users/plavrov/anaconda3/etc/profile.d/conda.sh" ]; then
        . "/Users/plavrov/anaconda3/etc/profile.d/conda.sh"
        CONDA_CHANGEPS1=false conda activate base
    else
        \export PATH="/Users/plavrov/anaconda3/bin:$PATH"
    fi
fi
unset __conda_setup
# <<< conda init <<<
